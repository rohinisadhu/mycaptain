#assigning elements to different lists
list1 = ['physics', 'chemistry', 1997, 2000];
list2 = [1, 2, 3, 4, 5, 6, 7 ];
list1.append(2020)
print(" ", list1)
list2.append(9)
print(" ", list2)

#accessing elements from tuple
my_tuple = (1, "Hello", 3.4)
print(my_tuple[0])

#deleting different dictionary elements
dict1={"1":"navin","2":"priya","3":"usha"}
dict2={"color":"pink","size":"small","weight":"43"}
del dict1["3"]
print(dict1)
del dict2["size"]
print(dict2)
